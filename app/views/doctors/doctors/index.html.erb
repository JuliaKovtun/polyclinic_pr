<%= link_to 'Profile', users_user_path(current_user) %>

<%= form_with(url: doctors_doctors_path, method: :get, local: true) do |f| %>
    <%= f.select :category_id, @categories.map{ |category| [category.title, category.id] }, {} %>
    <%= f.submit "find" %>
<% end %>

<%= render "doctors/doctors/doctors" %>